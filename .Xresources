*font:	                                    -xos4-terminus-medium-r-normal--12-120-72-72-c-60-iso10646-1
*savelines:	                                5000
*loginshell:	                              true
*eightBitInput:	                            false
*scrollBar:	                                false
*cursorBlink:	                              true
xterm*utf8:	                                1
xterm*vt100.geometry:	                      80x25
xterm*charClass:	                          33:48,35:48,37:48,43:48,45-47:48,64:48,95:48,126:48
xterm*termName:	                            xterm-256color

xterm*selectToClipboard:	                  true
xterm*VT100.Translations:	                  #override \n\
  Ctrl Shift <Key>V:                        insert-selection(CLIPBOARD) \n\
  Ctrl Shift <Key>C:                        copy-selection(CLIPBOARD)

urxvt.perl-ext-common:                      default,clipboard,keyboard-select
urxvt.keysym.M-escape:                      perl:keyboard-select
urxvt.keysym.M-c:                           perl:keyboard-copy
urxvt.keysym.C-v:                           perl:keyboard-paste
urxvt.keysym.M-C-v:                         perl:keyboard-paste_escaped
urxvt.clipboard.copycmd:                    xsel -ib
urxvt.clipboard.pastecmd:                   xsel -ob

urxvt*transparent:                          1
urxvt*internalBorder:                       10
urxvt*iso14755:                             false

xft.antialias:                              1
xft.hinting:                                1
xft.hintstyle:                              hintslight
xft.lcdfilter:                              lcddefault
xft.rgba:                                   rgb
xft.dpi:                                    96

xcursor.theme:                              Human

xscreensaver.Dialog.Button.background:      #444
xscreensaver.Dialog.Button.foreground:      #ededff
xscreensaver.Dialog.background:             #202020
xscreensaver.Dialog.borderWidth:            0
xscreensaver.Dialog.bottomShadowColor:      #202024
xscreensaver.Dialog.foreground:             #ededff
xscreensaver.Dialog.internalBorderWidth:    24
xscreensaver.Dialog.shadowThickness:        2
xscreensaver.Dialog.text.background:        #444
xscreensaver.Dialog.text.foreground:        #ededff
xscreensaver.Dialog.text.font:              -misc-roboto-medium-r-normal--17-120-100-100-p-0-iso10646-1
xscreensaver.Dialog.topShadowColor:         #202024
xscreensaver.dateFormat:                    %I:%M%P %a %b %d, %Y
xscreensaver.passwd.thermometer.background: #202020
xscreensaver.passwd.thermometer.foreground: #a9b7c4
xscreensaver.passwd.thermometer.width:      8

lal*font:                                   roboto
lal*fontsize:                               12

rofi.lines:                                 1
rofi.columns:                               1
rofi.eh:                                    1
rofi.bw:                                    1
rofi.location:                              0
rofi.width:                                 20
rofi.padding:                               15
rofi.opacity:                               95
rofi.fake-transparency:                     true
rofi.hide-scrollbar:                        true
rofi.disable-history:                       true
rofi.color-enabled:                         true
rofi.fullscreen:                            false
rofi.sidebar-mode:                          false
rofi.fuzzy:                                 false
rofi.separator-style:                       none
rofi.terminal:                              xterm
rofi.fake-background:                       background
rofi.font:                                  roboto 10
rofi.kb-clear-line:                         Control+b
rofi.color-normal:                          #1b1b1b, #777777, #1b1b1b, #777777, #d3d3d3
rofi.color-urgent:                          #1b1b1b, #777777, #1b1b1b, #777777, #d3d3d3
rofi.color-active:                          #1b1b1b, #777777, #1b1b1b, #777777, #d3d3d3
rofi.color-window:                          #1b1b1b, #7c6f64, #7c6f64

