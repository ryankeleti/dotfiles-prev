!! @ryankeleti ---  ~/.Xresources

xterm*font:	-xos4-terminus-medium-r-normal--12-120-72-72-c-60-iso10646-1
xterm*utf8:	1
xterm*savelines:	5000
xterm*loginshell:	true
xterm*vt100.geometry:	80x25
xterm*charClass:	33:48,35:48,37:48,43:48,45-47:48,64:48,95:48,126:48
xterm*termName:	xterm-256color
xterm*eightBitInput:	false
xterm*ScrollBar:	false
xterm*cursorBlink:	true

xterm*selectToClipboard:	true
xterm*VT100.Translations:	#override \n\
  Ctrl Shift <Key>V: insert-selection(CLIPBOARD) \n\
  Ctrl Shift <Key>C: copy-selection(CLIPBOARD)

! Gruvbox
*.background:            #282828
*.foreground:            #ebdbb2
*.fading:                40
*.shading:               50
*.fadeColor:             #282828
*.cursorColor:           #7c6f64
*.pointerColorBackground:#504945

*.color0:                #d5c4a1
*.color8:                #a89984
*.color1:                #fb4934
*.color9:                #fe8019
*.color2:                #98971a
*.color10:               #b8bb26
*.color3:                #fabd2f
*.color11:               #ebdbb2
*.color4:                #83a598
*.color12:               #bdae93
*.color5:                #928374
*.color13:               #d3869b
*.color6:                #8ec07c
*.color14:               #7c6f64
*.color7:                #fbf1c7
*.color15:               #d5c4a1

! rofi
rofi.lines: 1
rofi.columns: 1
rofi.eh: 1
rofi.location: 0
rofi.width: 20
rofi.padding: 15
rofi.fullscreen: false
rofi.opacity: 97
rofi.bw: 1
rofi.sidebar-mode: false
rofi.separator-style: none
rofi.hide-scrollbar: true
rofi.font: roboto 10
rofi.terminal: xterm
rofi.kb-clear-line: Control+b
rofi.disable-history: true
rofi.color-enabled: true
rofi.fuzzy: false
rofi.color-normal: #282828, #ebdbb2, #282828, #282828, #fabd2f  
rofi.color-urgent: #282828, #ebdbb2, #282828, #282828, #fabd2f  
rofi.color-active: #282828, #ebdbb2, #282828, #282828, #fabd2f  
rofi.color-window: #282828, #7c6f64, #7c6f64
