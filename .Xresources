! XTerm Resources [@ryankeleti]

xterm*font: -xos4-terminus-medium-r-normal--12-120-72-72-c-60-iso8859-*
XTerm*utf8: 1
xterm*savelines: 5000
xterm*loginshell: true
xterm*vt100*geometry: 80x25  
xterm*charClass: 33:48,35:48,37:48,43:48,45-47:48,64:48,95:48,126:48
xterm*termName: xterm-256color 
xterm*eightBitInput: false
XTerm*ScrollBar: false
xterm*cursorBlink: true

xterm*selectToClipboard: true
xterm*VT100.Translations: #override \
              Ctrl Shift <Key>V:    insert-selection(CLIPBOARD) \n\
              Ctrl Shift <Key>C:    copy-selection(CLIPBOARD)

#define base00 #1f1f1f 
#define base01 #d6d6d6
#define base02 #333333
#define base03 #eebb66
#define base05 #ffeeaa
#define base06 #eeffdd
#define base08 #ddaa55
#define base09 #99bfba

*.foreground:   base01
*.background:   base00
*.cursorColor:  base01

*.color0:       base03
*.color8:       base03

*.color1:       base05
*.color9:       base05

*.color2:       base06
*.color10:      base06

*.color3:       base08
*.color11:      base08

*.color4:       base05
*.color12:      base05

*.color5:       base09
*.color13:      base09

*.color6:       base05
*.color14:      base05

*.color7:       base02
*.color15:      base02

