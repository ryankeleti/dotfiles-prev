!! @ryankeleti ---  ~/.Xresources

xterm*font:	-xos4-terminus-medium-r-normal--12-120-72-72-c-60-iso10646-1
xterm*utf8:	1
xterm*savelines:	5000
xterm*loginshell:	true
xterm*vt100*geometry:	80x25
xterm*charClass:	33:48,35:48,37:48,43:48,45-47:48,64:48,95:48,126:48
xterm*termName:	xterm-256color
xterm*eightBitInput:	false
xterm*ScrollBar:	false
xterm*cursorBlink:	true

xterm*selectToClipboard:	true
xterm*VT100.Translations:	#override \n\
  Ctrl Shift <Key>V: insert-selection(CLIPBOARD) \n\
  Ctrl Shift <Key>C: copy-selection(CLIPBOARD)

! Gruvbox
#define G_base03        #282828
#define G_base02        #d5c4a1
#define G_base01        #504945
#define G_base00        #ebdbb2
#define G_base0         #bdae93
#define G_base1         #7c6f64
#define G_base2         #fbf1c7
#define G_base3         #d5c4a1
#define G_base4         #a89984
#define G_yellow        #fabd2f
#define G_orange        #fe8019
#define G_red           #fb4934
#define G_grey          #928374
#define G_purple        #d3869b
#define G_blue          #83a598
#define G_aqua          #8ec07c
#define G_green         #b8bb26

*.background:            G_base03
*.foreground:            G_base00
*.fading:                40
*.fadeColor:             G_base03
*.cursorColor:           G_base1
*.pointerColorBackground:G_base01

*.color0:                G_base02
*.color8:                G_base4
*.color1:                G_red
*.color9:                G_orange
*.color2:                G_green
*.color10:               G_base01
*.color3:                G_yellow
*.color11:               G_base00
*.color4:                G_blue
*.color12:               G_base0
*.color5:                G_grey
*.color13:               G_purple
*.color6:                G_aqua
*.color14:               G_base1
*.color7:                G_base2
*.color15:               G_base3

! rofi
rofi.lines: 1
rofi.columns: 1
rofi.eh: 1
rofi.location: 0
rofi.width: 20
rofi.padding: 15
rofi.fullscreen: false
rofi.opacity: 97
rofi.bw: 1
rofi.sidebar-mode: false
rofi.separator-style: none
rofi.hide-scrollbar: true
rofi.font: roboto 10
rofi.terminal: xterm
rofi.kb-clear-line: Control+b
rofi.disable-history: true
rofi.color-enabled: true
rofi.fuzzy: false
rofi.color-normal: G_base03, G_base00, G_base03, G_base03, G_yellow  
rofi.color-urgent: G_base03, G_base00, G_base03, G_base03, G_yellow  
rofi.color-active: G_base03, G_base00, G_base03, G_base03, G_yellow  
rofi.color-window: G_base03, G_base1, G_base1
