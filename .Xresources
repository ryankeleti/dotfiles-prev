urxvt.transparent:        0
urxvt.internalBorder:     10
urxvt.depth:              32
urxvt.shading:            10
urxvt.iso14755:           0

urxvt.colorIT:            #87afd7
urxvt.colorBD:            #d3d3d3
urxvt.colorUL:            #87afd7

urxvt.keysym.S-Up:        \033[1;2A
urxvt.keysym.S-Down:      \033[1;2B
urxvt.keysym.S-Left:      \033[1;5D
urxvt.keysym.S-Right:     \033[1;5C

*font:	                  -xos4-terminus-medium-r-normal--12-120-72-72-c-60-iso10646-1
*savelines:	              5000
*loginshell:	            true
*eightBitInput:	          false
*scrollBar:	              0
*cursorBlink:	            1
xterm*utf8:	              1
xterm*vt100.geometry:	    80x25
xterm*charClass:	        33:48,35:48,37:48,43:48,45-47:48,64:48,95:48,126:48
xterm*termName:	          xterm-256color

xterm*selectToClipboard:	true
xterm*VT100.Translations:	#override \n\
  Ctrl Shift <Key>V:      insert-selection(CLIPBOARD) \n\
  Ctrl Shift <Key>C:      copy-selection(CLIPBOARD)

xft.autohint:             0  
xft.antialias:            1
xft.hinting:              1
xft.hintstyle:            hintslight
xft.lcdfilter:            lcddefault
xft.rgba:                 rgb
xft.dpi:                  96

xcursor.theme:            Human
xscreensaver.dateFormat:  %a %b %d, %Y

lal*font:                 roboto
lal*fontsize:             12

rofi.lines:               1
rofi.columns:             1
rofi.location:            0
rofi.width:               20
rofi.fake-transparency:   true
rofi.hide-scrollbar:      true
rofi.disable-history:     true
rofi.color-enabled:       true
rofi.fuzzy:               false
rofi.separator-style:     none
rofi.terminal:            xterm
rofi.font:                roboto 10
rofi.kb-clear-line:       Control+b
rofi.color-normal:        dark, zc07, dark, gold, dark
rofi.color-active:        dark, zc07, dark, gold, dark
rofi.color-urgent:        dark, zc07, dark, gold, #cf6a4c
rofi.color-window:        dark, dark, dark

